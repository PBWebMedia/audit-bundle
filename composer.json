{
  "name": "pbweb/audit-bundle",
  "type": "symfony-bundle",
  "description": "PB Web Media Audit Bundle for Symfony",
  "keywords": [
    "audit",
    "bundle",
    "symfony"
  ],
  "license": "MIT",
  "require": {
    "php": "^7.0",
    "psr/log": "^1.0",
    "symfony/symfony": "^3.2"
  },
  "require-dev": {
    "phpunit/phpunit": "^5.6",
    "doctrine/orm": "^2.5",
    "mockery/mockery": "^0.9.6",
    "sebastian/phpcpd": "^2.0",
    "phploc/phploc": "^3.0",
    "mayflower/php-codebrowser": "^1.1"
  },
  "autoload": {
    "psr-4": {
      "Pbweb\\AuditBundle\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\Pbweb\\AuditBundle\\": "tests/"
    }
  },
  "config": {
    "bin-dir": "bin"
  },
  "suggest": {
    "doctrine/orm": "For writing the audit log to the database",
    "symfony/security": "For if you use the default event appenders"
  },
  "extra": {
    "branch-alias": {
      "dev-master": "1.x-dev"
    }
  }
}
