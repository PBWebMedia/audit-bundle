{
  "name": "pbweb/audit-bundle",
  "type": "symfony-bundle",
  "description": "PB Web Media Audit Bundle for Symfony",
  "keywords": [
    "audit",
    "bundle",
    "symfony"
  ],
  "license": "MIT",
  "require": {
    "php": ">=8.0",
    "ext-json": "*",
    "psr/log": "^1.0",
    "symfony/http-kernel": "^4.4",
    "symfony/dependency-injection": "^4.4",
    "symfony/event-dispatcher": "^4.4",
    "symfony/config": "^4.4",
    "symfony/yaml": "^4.4"
  },
  "require-dev": {
    "phpunit/phpunit": "^9.5",
    "mockery/mockery": "^1.4",
    "doctrine/orm": "^2.9",
    "symfony/framework-bundle": "^4.4",
    "symfony/security-bundle": "^4.4",
    "symfony/monolog-bundle": "^3.7"
  },
  "autoload": {
    "psr-4": {
      "Pbweb\\AuditBundle\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\Pbweb\\AuditBundle\\": "tests/"
    }
  },
  "config": {
    "bin-dir": "bin"
  },
  "suggest": {
    "doctrine/orm": "For writing the audit log to the database",
    "symfony/security": "For if you use the default event appenders"
  },
  "extra": {
    "branch-alias": {
      "dev-master": "2.x-dev"
    }
  }
}
